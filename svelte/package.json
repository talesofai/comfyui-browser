{
  "name": "comfyui-browser",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "extract": "svelte-i18n extract './src/**/*.svelte' ./src/i18n/zh-CN.json && svelte-i18n extract './src/**/*.svelte' ./src/i18n/en-US.json"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^20.11.28",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.22",
    "postcss": "^8.4.32",
    "sass": "^1.72.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "type": "module",
  "dependencies": {
    "dayjs": "^1.11.10",
    "svelte-i18n": "^4.0.0"
  }
}